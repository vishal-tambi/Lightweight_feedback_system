{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    { "source": "/terms-of-service.html", "destination": "/terms-of-service.html" },
    { "source": "/privacy-policy.html", "destination": "/privacy-policy.html" },
    { "source": "/copyright-notice.html", "destination": "/copyright-notice.html" },
    { "source": "/robots.txt", "destination": "/robots.txt" },
    { "source": "/vite.svg", "destination": "/vite.svg" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "env": {
    "VITE_API_URL": "https://your-backend-url.com"
  }
} 